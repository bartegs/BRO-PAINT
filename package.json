  "main": "index.js",
  "scripts": {
    "dev:watch": "webpack serve --open --config webpack.dev.js ",
    "dev:server": "nodemon",
    "qc": "run-p lint:fix check-types test",
    "test": "jest",
    "lint:ts": "eslint --ext tsx --ext ts .",
    "lint:ts:fix": "eslint --fix --ext tsx --ext ts .",
    "lint:scss": "stylelint  apps/**/*.scss",
    "lint:scss:fix": "stylelint apps/**/*.scss --fix",
    "lint": "run-p lint:ts lint:scss",
    "lint:fix": "run-p lint:ts:fix lint:scss:fix",
    "check-types": "tsc -p tsconfig.json --noEmit",
    "prepare": "husky install",
    "build:server": "tsc server/server.ts --outDir ./dist/server",
    "build:apps:dev": "webpack --config webpack.dev.js",
    "build:apps:prod": "webpack --config webpack.prod.js",
    "build:prod": "run-p build:apps:prod build:server",
    "start": "node dist/server/server.js",
    "heroku-postbuild": "run-p build:prod"
  },
